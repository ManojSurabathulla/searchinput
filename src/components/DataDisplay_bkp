import React from "react";

const DataDisplay = (props) => {
  const renderList = props.datalist.map((data) => {
    return (
      <div>
        <h3>{data.name}</h3>
      </div>
    );
  });
  return <div>{renderList}</div>;
};
export default DataDisplay;

